@model JqueryForm.Models.Emp
<tr class="new">
    <th scope="row"></th>
    <td>
        @Html.DisplayFor(model => model.EmpID)
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @*@Html.HiddenFor(model => model.EmpID)*@
        <input type="hidden" class="EmpID" id="EmpID" name="EmpID" value="@Model.EmpID" />
    </td>
    <td>
        @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control input-sm name" } })
        @Html.ValidationMessageFor(model => model.Name, "", new { @class = "text-danger" })
    </td>
    <td>
        @Html.EditorFor(model => model.Salary, new { htmlAttributes = new { @class = "form-control input-sm salary" } })
        @Html.ValidationMessageFor(model => model.Salary, "", new { @class = "text-danger" })
    </td>
    <td>
        @Html.EditorFor(model => model.DOB, new { htmlAttributes = new { @class = "form-control input-sm dob" } })
        @Html.ValidationMessageFor(model => model.DOB, "", new { @class = "text-danger" })
    </td>
    <td>
        @Html.EditorFor(model => model.Age, new { htmlAttributes = new { @class = "form-control input-sm age" } })
        @Html.ValidationMessageFor(model => model.Age, "", new { @class = "text-danger" })
    </td>
    <td>
        @Html.DropDownListFor(model => model.Language, new List<SelectListItem>
        {
            new SelectListItem{ Text="C#", Value = "C#" },
            new SelectListItem{ Text="Python", Value = "Python" },
            new SelectListItem{ Text="JavaScript", Value = "JavaScript" },
            new SelectListItem{ Text="JQuery", Value = "JQuery" },
         }, new { @class = "form-control input-sm language" })
        @Html.ValidationMessageFor(model => model.Language, "", new { @class = "text-danger" })
    </td>
    <th scope="col"><input onclick="EditSubmit(this)" type="button" value="Update" class="btn btn-default btn-xs" /></th>
</tr>
